{
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "docs-html",
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/test/**",
    "**/__tests__/**"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "includeVersion": true,
  "readme": "README.md",
  "tsconfig": "tsconfig.json",
  "name": "Modern React Template Documentation",
  "navigationLinks": {
    "GitHub": "https://github.com/asudbury/modern-react-template"
  }
}
